
main()
{
NODE* head=NULL;
int ch;
while(1)
{
printf("Enter choice\n");
scanf("%d",&ch);
switch(ch)
{
  case 1:head=insert(head);
          break;
  case 2: head=delete(head);
           break;
  case 3: print(head);break;
  case 4: head=reverse(head);
  case 5: head=sort(head);break;
  default: printf("Invalid choice\n");

}
}
}
